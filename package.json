{
  "name": "angular-2-demo",
  "description": "The demo for Angular 2 with Docker Container",
  "version": "0.0.2",
  "readme": "README.md",
  "engines": {
    "node": "6.9.1"
  },
  "repository": {
    "github": ""
  },
  "scripts": {
    "start-server": "concurrently \"npm run tsc:w\" \"node server/server.js\" ",
    "tsc:w": "tsc -w",
	"clean": "rm -rf dist",
	"build": "tsc",
	"rollup": "rollup -f iife -c rollup.config.js -o dist/bundle.es2015.js",
	"es5": "tsc --target es5 --allowJs dist/bundle.es2015.js --out dist/bundle.js",
	"minify": "uglifyjs dist/bundle.js --screw-ie8 --compress --mangle --output dist/bundle.min.js",
	"build_prod": "npm run clean && npm run build && browserify -s main dist/app.module.js > dist/bundle.js && npm run minify"
  },
  "dependencies": {
    "@angular/common": ">2.0.0",
    "@angular/compiler": ">2.0.0",
    "@angular/core": ">2.0.0",
    "@angular/forms": ">2.0.0",
    "@angular/http": ">2.0.0",
    "@angular/platform-browser": ">2.0.0",
    "@angular/platform-browser-dynamic": ">2.0.0",
    "@angular/router": ">3.0.0",
    "body-parser": "^1.8.2",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "express": "^4.9.3",
    "mongoose": "^4.4.5",
    "morgan": "^1.1.1",
    "ng2-charts": "^1.0.0-beta.0",
    "reflect-metadata": "^0.1.3",
    "rxjs": "5.0.0-beta.12",
    "sentiment": "^1.0.6",
    "serve-favicon": "^2.0.1",
    "systemjs": "0.19.27",
    "twitter": "^1.2.5",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/angular": "^1.5.19",
    "@types/core-js": "^0.9.34",
    "concurrently": "^2.0.0",
    "del": "^2.2.0",
    "gulp": ">0.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-minify": ">0.0.0",
    "lite-server": "^2.1.0",
    "tslint": "^3.15.1",
    "vinyl-paths": "^2.1.0",
    "@angular/compiler-cli": "^2.1.0",
    "@angular/platform-server": "^2.1.0",
    "browserify": "^13.0.1",
    "http-server": "^0.9.0",
    "rollup": "^0.36.3",
    "rollup-plugin-node-resolve": "^2.0.0",
    "typescript": "^2.0.3",
    "uglifyjs": "^2.4.10"
  }
}
